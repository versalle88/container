{
    "name": "versalle/container",
    "description": "A PSR-11: Container interface implementation library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Versalle",
            "email": "andrew.versalle@gmail.com"
        }
    ],
    "require": {
        "php": "7.2.* || 7.3.* || 7.4.*",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.6",
        "phpspec/phpspec": "^6.1",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": "^3.8"
    },
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Versalle\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/Fixtures/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.28"
        },
        "sort-packages": true
    }
}
